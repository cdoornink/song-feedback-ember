<div class="row-fluid">
  {{#with SF.loginController.content}}
  <h3 class="pull-left">{{firstName}} {{lastName}}</h3>
  <p class="pull-left my-songs-upload-button"><a class="btn" href="#/upload">Upload Song</a></p>
  {{/with}}
</div>
<div class="row-fluid">
{{#each song in controller.songs}}
  {{view SF.SongListItemView contentBinding='song'}}
{{/each}}
</div>